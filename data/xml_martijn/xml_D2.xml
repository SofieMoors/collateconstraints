<text n="Eerste Martijn" xml:id="M1">
<body>
<p><lb n="1" xml:id="D2f1r.1"/><l n="966"><hi rend="capitalsize1">I</hi>c vergheuet al onghedo<damage><gap></gap></damage></l>
<lb n="2" xml:id="D2f1r.2"/><l n="967"><hi rend="capitalsize1">A</hi>lle <choice><abbr>vrouwe<g ref="#bar"/></abbr><expan>vrouwe<ex>n</ex></expan></choice> ionc <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> oud <damage><gap></gap></damage></l>
<lb n="3" xml:id="D2f1r.3"/><l n="968"><hi rend="capitalsize1">O</hi>m eene vrouwe hogh<damage><gap></gap></damage></l>
<lb n="4" xml:id="D2f1r.4"/><l n="969"><hi rend="capitalsize1">D</hi>aer ons afquam onse <damage><gap></gap></damage></l>
<lb n="5" xml:id="D2f1r.5"/><l n="970"><hi rend="capitalsize1">G</hi>hedanct si die dusentfo<damage><gap></gap></damage></l>
<lb n="6" xml:id="D2f1r.6"/><l n="971"><hi rend="capitalsize1">D</hi>atse hem op hielt met <damage><gap></gap></damage></l>
<lb n="7" xml:id="D2f1r.7"/><l n="972"><hi rend="capitalsize1">D</hi>ie ons benam thelsch <damage><gap></gap></damage></l>
<lb n="8" xml:id="D2f1r.8"/><l n="973"><hi rend="capitalsize1">I</hi>n haer ben ic te <choice><abbr>eynde<g ref="#bar"/></abbr><expan>eynde<ex>n</ex></expan></choice> b<damage><gap></gap></damage></l>
<lb n="9" xml:id="D2f1r.9"/><l n="974"><hi rend="capitalsize1">O</hi>nser beider dyaloghe</l>
<lb n="10" xml:id="D2f1r.10"/><l n="975"><hi rend="capitalsize1">I</hi>c en vrage niet meer no<damage><gap></gap></damage></l>
</p>
</body>
</text>


<text n="Tweede Martijn" xml:id="M2">
<body>
<p><lb n="11" xml:id="D2f1r.11"/><l n="000">§ <hi rend="capitalsize1">D</hi>at ander de bo<damage><gap></gap></damage></l>
<lb n="12" xml:id="D2f1r.12"/><l n="001"><hi rend="capitalsize2">M</hi>Artijn slaepty sla<damage><gap></gap></damage></l>
<lb n="13" xml:id="D2f1r.13"/><l n="002"><hi rend="capitalsize1">S</hi>preect hebdy g<damage><gap></gap></damage></l>
<lb n="14" xml:id="D2f1r.14"/><l n="003"><hi rend="capitalsize1">G</hi>hi duncket mi verdor<damage><gap></gap></damage></l>
<lb n="15" xml:id="D2f1r.15"/><l n="004"><hi rend="capitalsize1">G</hi>hi en achtet meer noch<damage><gap></gap></damage></l>
<lb n="16" xml:id="D2f1r.16"/><l n="005"><hi rend="capitalsize1">D</hi>an op weelde <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> ghe <damage><gap></gap></damage></l>
<lb n="17" xml:id="D2f1r.17"/><l n="006"><hi rend="capitalsize1">W</hi>ildi v dus versmoren <damage><gap></gap></damage></l>
<lb n="18" xml:id="D2f1r.18"/><l n="007"><hi rend="capitalsize1">A</hi>l waert al uwe dat coe<damage><gap></gap></damage></l>
<lb n="19" xml:id="D2f1r.19"/><l n="008"><hi rend="capitalsize1">G</hi>out silner loot stael ys<damage><gap></gap></damage></l>
<lb n="20" xml:id="D2f1r.20"/><l n="009"><hi rend="capitalsize1">N</hi>ochtan bleeft ghi verl<damage><gap></gap></damage></l>
<lb n="21" xml:id="D2f1r.21"/><l n="010"><hi rend="capitalsize1">D</hi>aerstu <choice><abbr>d<g ref="#apomod"/></abbr><expan>d<ex>er</ex></expan></choice> duecht <choice><abbr>gee<g ref="#bar"/></abbr><expan>gee<ex>n</ex></expan></choice> be<damage><gap></gap></damage></l>


<pb xml:id="D2.f1v" n="1v"/>
<lb n="1" xml:id="D2f1v.1"/><l n="011"><damage><gap></gap></damage> hooft <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> uwen sin</l>
<lb n="2" xml:id="D2f1v.2"/><l n="012"><damage><gap></gap></damage> waerstu geboren</l>
<lb n="3" xml:id="D2f1v.3"/><l n="013"><damage><gap></gap></damage>chtens versworen</l>
<lb n="4" xml:id="D2f1v.4"/><l n="014"><damage><gap></gap></damage>ghi woent in den dam</l>
<lb n="5" xml:id="D2f1v.5"/><l n="015"><damage><gap></gap></damage> echt dies ben ic gram</l>
<lb n="6" xml:id="D2f1v.6"/><l n="016"><damage><gap></gap></damage>s sijn verscheeden</l>
<lb n="7" xml:id="D2f1v.7"/><l n="017"><damage><gap></gap></damage>at mi meest mesquam</l>
<lb n="8" xml:id="D2f1v.8"/><l n="018"><damage><gap></gap></damage>t bate en vernam</l>
<lb n="9" xml:id="D2f1v.9"/><l n="019"><damage><gap></gap></damage> en <choice><abbr>va<g ref="#bar"/></abbr><expan>va<ex>n</ex></expan></choice> ons beeden</l>
<lb n="10" xml:id="D2f1v.10"/><l n="020"><damage><gap></gap></damage>en sin gheworden so lam</l>
<lb n="11" xml:id="D2f1v.11"/><l n="021"><damage><gap></gap></damage>node <choice><abbr>wed<g ref="#apomod"/></abbr><expan>wed<ex>er</ex></expan></choice> ane quam</l>
<lb n="12" xml:id="D2f1v.12"/><l n="022"><damage><gap></gap></damage> mi verleeden</l>
<lb n="13" xml:id="D2f1v.13"/><l n="023"><damage><gap></gap></damage> mi als een gestoten ram</l>
<lb n="14" xml:id="D2f1v.14"/><l n="024"><damage><gap></gap></damage> ghi wort noch tam</l>
<lb n="15" xml:id="D2f1v.15"/><l n="025"><damage><gap></gap></damage> woorden cleeden</l>
<lb n="16" xml:id="D2f1v.16"/><l n="026"><damage><gap></gap></damage> <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> met reden</l>
<lb n="17" xml:id="D2f1v.17"/><l n="027"><damage><gap></gap></damage> vrient heb ic messeyt</l>
<lb n="18" xml:id="D2f1v.18"/><l n="028"><damage><gap></gap></damage>eteren bereyt</l>
<lb n="19" xml:id="D2f1v.19"/><l n="029"><damage><gap></gap></damage> v bliuen</l>
<lb n="20" xml:id="D2f1v.20"/><l n="030"><damage><gap></gap></damage> et mi <choice><abbr>eu<g ref="#bar"/></abbr><expan>eu<ex>de</ex></expan></choice> ontbeyt</l>
<lb n="21" xml:id="D2f1v.21"/><l n="031"><damage><gap></gap></damage>mi opt he<unclear>r</unclear>tte leyt</l>


<pb xml:id="D2.f2r" n="2r"/>
<lb n="1" xml:id="D2f2r.1"/><l n="032"><hi rend="capitalsize1">D</hi>at wil ic v bescriuen</l>
<lb n="2" xml:id="D2f2r.2"/><l n="033"><hi rend="capitalsize1">D</hi>aer haer die werelt mede m<damage><gap></gap></damage></l>
<lb n="3" xml:id="D2f2r.3"/><l n="034"><hi rend="capitalsize1">E</hi>nde dade dat lief ware leyt</l>
<lb n="4" xml:id="D2f2r.4"/><l n="035"><hi rend="capitalsize1">D</hi>ats minne <choice><abbr>va<g ref="#bar"/></abbr><expan>va<ex>n</ex></expan></choice> wiuen</l>
<lb n="5" xml:id="D2f2r.5"/><l n="036"><hi rend="capitalsize1">A</hi>l dat is inde werelt breyt</l>
<lb n="6" xml:id="D2f2r.6"/><l n="037"><choice><abbr><hi rend="capitalsize1">S</hi>teden/borge<g ref="#bar"/>/bossce<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">S</hi>teden/borge<ex>n</ex>/bossce<ex>n</ex></expan></choice> preyt</l>
<lb n="7" xml:id="D2f2r.7"/><l n="038"><hi rend="capitalsize1">S</hi>oudemen verdriuen</l>
<lb n="8" xml:id="D2f2r.8"/><l n="039"><hi rend="capitalsize1">W</hi>ildemen minne ontlijuen</l>
<lb n="9" xml:id="D2f2r.9"/><l n="040"><hi rend="capitalsize1">I</hi>acop dit was oyt dijn doen</l>
<lb n="10" xml:id="D2f2r.10"/><l n="041"><hi rend="capitalsize1">D</hi>at van vrouwen v sermoen</l>
<lb n="11" xml:id="D2f2r.11"/><l n="042"><hi rend="capitalsize1">B</hi>eghinnen moet oft eynden</l>
<lb n="12" xml:id="D2f2r.12"/><l n="043"><hi rend="capitalsize1">W</hi>aerdi een dorper ghi spraec <damage><gap></gap></damage></l>
<lb n="13" xml:id="D2f2r.13"/><l n="044"><hi rend="capitalsize1">N</hi>v es v herte dus ontuloen</l>
<lb n="14" xml:id="D2f2r.14"/><l n="045"><hi rend="capitalsize1">D</hi>och wil icx mi ghenenden</l>
<lb n="15" xml:id="D2f2r.15"/><l n="046"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> strijcken af minen caproen</l>
<lb n="16" xml:id="D2f2r.16"/><l n="047"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> horen wat v hier toe mach <damage><gap></gap></damage></l>
<lb n="17" xml:id="D2f2r.17"/><l n="048"><hi rend="capitalsize1">D</hi>at ghi den onbekenden</l>
<lb n="18" xml:id="D2f2r.18"/><l n="049"><hi rend="capitalsize1">M</hi>artijn niet en laet gheuroen</l>
<lb n="19" xml:id="D2f2r.19"/><l n="050"><hi rend="capitalsize1">A</hi>l hadt ic aen twee yseren scho<damage><gap></gap></damage></l>
<lb n="20" xml:id="D2f2r.20"/><l n="051"><hi rend="capitalsize1">I</hi>c en mocht v niet ontwenden</l>
<lb n="21" xml:id="D2f2r.21"/><l n="052"><hi rend="capitalsize1">N</hi>v vraecht ic sal <choice><abbr>ontbende<g ref="#bar"/></abbr><expan>ontbende<ex>n</ex></expan></choice></l>


<pb xml:id="D2.f2v" n="2v"/>
<lb n="1" xml:id="D2f2v.1"/><l n="053"><damage><gap></gap></damage>rtijn ghi sult mi verstaen</l>
<lb n="2" xml:id="D2f2v.2"/><l n="054"><damage><gap></gap></damage> vrouwe wel ghedaen</l>
<lb n="3" xml:id="D2f2v.3"/><l n="055"><damage><gap></gap></damage>t mi beuaen met minnen</l>
<lb n="4" xml:id="D2f2v.4"/><l n="056"><damage><gap></gap></damage>t gode is onderdaen</l>
<lb n="5" xml:id="D2f2v.5"/><l n="057"><damage><gap></gap></damage> om haren wille gaen</l>
<lb n="6" xml:id="D2f2v.6"/><l n="058"><damage><gap></gap></damage>ht icse ghewinnen</l>
<lb n="7" xml:id="D2f2v.7"/><l n="059"><damage><gap></gap></damage>tan weetic <choice><abbr>sond<g ref="#apomod"/></abbr><expan>sond<ex>er</ex></expan></choice> waen</l>
<lb n="8" xml:id="D2f2v.8"/><l n="060"><damage><gap></gap></damage>ij <choice><abbr>mij<g ref="#bar"/>s</abbr><expan>mij<ex>n</ex>s</expan></choice> niet en acht een spaen</l>
<lb n="9" xml:id="D2f2v.9"/><l n="061"><damage><gap></gap></damage>n hert doet mi bekennen</l>
<lb n="10" xml:id="D2f2v.10"/><l n="062"><damage><gap></gap></damage>uder die werelt om vergaen</l>
<lb n="11" xml:id="D2f2v.11"/><l n="063"><damage><gap></gap></damage> si mi niet en sonde ontfaen</l>
<lb n="12" xml:id="D2f2v.12"/><l n="064"><damage><gap></gap></damage>aer herte binnen</l>
<lb n="13" xml:id="D2f2v.13"/><l n="065"><damage><gap></gap></damage>etoe en is geen versinnen</l>
<lb n="14" xml:id="D2f2v.14"/><l n="066"><damage><gap></gap></damage>n <choice><abbr>and<g ref="#apomod"/></abbr><expan>and<ex>er</ex></expan></choice> vrouwe die mi bemint</l>
<lb n="15" xml:id="D2f2v.15"/><l n="067"><damage><gap></gap></damage>en al dat sij leuende kint</l>
<lb n="16" xml:id="D2f2v.16"/><l n="068"><damage><gap></gap></damage> ghestadiger trouwen</l>
<lb n="17" xml:id="D2f2v.17"/><l n="069"><damage><gap></gap></damage>in mi gheprijst es twint</l>
<lb n="18" xml:id="D2f2v.18"/><l n="070"><damage><gap></gap></damage>er si is die scoonste <choice><abbr>dieme<g ref="#bar"/></abbr><expan>dieme<ex>n</ex></expan></choice> vindt</l>
<lb n="19" xml:id="D2f2v.19"/><l n="071"><damage><gap></gap></damage><unclear>b</unclear>loeme bouen alle <choice><abbr>vrouwe<g ref="#bar"/></abbr><expan>vrouwe<ex>n</ex></expan></choice></l>
<lb n="20" xml:id="D2f2v.20"/><l n="072"><damage><gap></gap></damage>es <choice><abbr>mij<g ref="#bar"/></abbr><expan>mij<ex>n</ex></expan></choice> herte soe verblint</l>
<lb n="21" xml:id="D2f2v.21"/><l n="073"><damage><gap></gap></damage> <choice><abbr>he<g ref="#bar"/></abbr><expan>he<ex>m</ex></expan></choice> daertoe niet en verbint</l>


<pb xml:id="D2.f3r" n="3r"/>
<lb n="1" xml:id="D2f3r.1"/><l n="074"><hi rend="capitalsize1">D</hi>at icse yet wille scouwen</l>
<lb n="2" xml:id="D2f3r.2"/><l n="075"><hi rend="capitalsize1">H</hi>oe lie<unclear>f</unclear> opsien <pc>/</pc> si mi toe sindt</l>
<lb n="3" xml:id="D2f3r.3"/><l n="076"><hi rend="capitalsize1">H</hi>et dunct <choice><abbr>mijnd<g ref="#apomod"/></abbr><expan>mijnd<ex>er</ex></expan></choice> <choice><abbr>herte<g ref="#bar"/></abbr><expan>herte<ex>n</ex></expan></choice> alsen <damage><gap></gap></damage></l>
<lb n="4" xml:id="D2f3r.4"/><l n="077"><hi rend="capitalsize1">S</hi>ij liet h<unclear>a</unclear>er eer in stucken hou<damage><gap></gap></damage></l>
<lb n="5" xml:id="D2f3r.5"/><l n="078"><hi rend="capitalsize1">E</hi>er sij daer om quam in rouw<damage><gap></gap></damage></l>
<lb n="6" xml:id="D2f3r.6"/><l n="079"><hi rend="capitalsize1">M</hi>artijn hoort <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> verstant</l>
<lb n="7" xml:id="D2f3r.7"/><l n="080"><hi rend="capitalsize1">D</hi>ese twee sijn in een verbant</l>
<lb n="8" xml:id="D2f3r.8"/><l n="081"><hi rend="capitalsize1">D</hi>at costen moet der eenredle <damage><gap></gap></damage></l>
<lb n="9" xml:id="D2f3r.9"/><l n="082"><hi rend="capitalsize1">N</hi>v heb ic die macht in die ha<damage><gap></gap></damage></l>
<lb n="10" xml:id="D2f3r.10"/><l n="083"><hi rend="capitalsize1">W</hi>elcker ic wille breken den <damage><gap></gap></damage></l>


<pb xml:id="D2.f3v" n="3v"/>
<lb n="1" xml:id="D2f3v.1"/><l n="095"><damage><gap></gap></damage> ymmer der herten aert</l>
<lb n="2" xml:id="D2f3v.2"/><l n="096"><damage><gap></gap></damage>darwaerts tijdt <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> vaert</l>
<lb n="3" xml:id="D2f3v.3"/><l n="097"><damage><gap></gap></damage>aen haer <choice><abbr>quame<g ref="#bar"/></abbr><expan>quame<ex>n</ex></expan></choice> d<unclear>ie</unclear> wonden</l>
<lb n="4" xml:id="D2f3v.4"/><l n="098"><damage><gap></gap></damage>t al varen ghinder<unclear>w</unclear>aert</l>
<lb n="5" xml:id="D2f3v.5"/><l n="099"><damage><gap></gap></damage>el varens begaert</l>
<lb n="6" xml:id="D2f3v.6"/><l n="100"><damage><gap></gap></damage> tot allen stonden</l>
<lb n="7" xml:id="D2f3v.7"/><l n="101"><damage><gap></gap></damage>e die mi therte beswaert</l>
<lb n="8" xml:id="D2f3v.8"/><l n="102"><damage><gap></gap></damage>ick haer <choice><abbr>late<g ref="#bar"/></abbr><expan>late<ex>n</ex></expan></choice> <choice><abbr>smake<g ref="#bar"/></abbr><expan>smake<ex>n</ex></expan></choice> tswaert</l>
<lb n="9" xml:id="D2f3v.9"/><l n="103"><choice><abbr><g ref="#apomod"/></abbr><expan><ex>.</ex></expan></choice> <pc>.</pc> .are dan ondervonden</l>
<lb n="10" xml:id="D2f3v.10"/><l n="104"><damage><gap></gap></damage>ermoort dan sonden</l>


<pb xml:id="D2.f4r" n="4r"/>
<lb n="1" xml:id="D2f4r.1"/><l n="116"><hi rend="capitalsize1">H</hi>i en konst hem niet bed<unclear>w</unclear><damage><gap></gap></damage></l>
<lb n="2" xml:id="D2f4r.2"/><l n="117"><hi rend="capitalsize1">H</hi>i en sterf bi desen dingen</l>
<lb n="3" xml:id="D2f4r.3"/><l n="118"><hi rend="capitalsize1">M</hi>edea toocht ons selue de <damage><gap></gap></damage></l>
<lb n="4" xml:id="D2f4r.4"/><l n="119"><hi rend="capitalsize1">D</hi>ie roofde <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> stal haers v<damage><gap></gap></damage></l>
<lb n="5" xml:id="D2f4r.5"/><l n="120"><choice><abbr><hi rend="capitalsize1">E</hi>n<g ref="#bar"/></abbr><expan><hi rend="capitalsize1">E</hi>n<ex>de</ex></expan></choice> versloech haren broed<damage><gap></gap></damage></l>
<lb n="6" xml:id="D2f4r.6"/><l n="121"><hi rend="capitalsize1">O</hi>m iason dies haer cume<damage><gap></gap></damage></l>
<lb n="7" xml:id="D2f4r.7"/><l n="122"><hi rend="capitalsize1">L</hi>iet sij conincrijc <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> s<unclear>tadt</unclear></l>
<lb n="8" xml:id="D2f4r.8"/><l n="123"><hi rend="capitalsize1">V</hi>ader suster <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> m<damage><gap></gap></damage></l>
<lb n="9" xml:id="D2f4r.9"/><l n="124"><hi rend="capitalsize1">N</hi>ochtans daer sij<damage><gap></gap></damage>e<damage><gap></gap></damage></l>
<lb n="10" xml:id="D2f4r.10"/><l n="125"><hi rend="capitalsize1">W</hi>ijsde haer red<unclear>e</unclear><damage><gap></gap></damage></l>


<pb xml:id="D2.f4v" n="4v"/>
<lb n="1" xml:id="D2f4v.1"/><l n="137"><damage><gap></gap></damage>t woent <choice><abbr>sond<g ref="#apomod"/></abbr><expan>sond<ex>er</ex></expan></choice> ghetal</l>
<lb n="2" xml:id="D2f4v.2"/><l n="138"><damage><gap></gap></damage>berch <choice><abbr>en<g ref="#bar"/></abbr><expan>en<ex>de</ex></expan></choice> in dal</l>
<lb n="3" xml:id="D2f4v.3"/><l n="139"><damage><gap></gap></damage>c hertoge of graue</l>
